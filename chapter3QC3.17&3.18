class repeated {
    
public static int repeatedArr(int[] arr) { 
int c1 = 0, c2 = 0; 
for(int i = 0; i < arr.length; i++) { 
 c1 = arr[i]; 
  for(int j = 0; j < arr.length; j++) { 
        if(j == i) { 
    	continue; 
        }else if(arr[j] == c1) { 
          c2 = arr[j]; 
          return c2; 
       } 
     } 
   } 
return 0; 
} 

public static int repeatedF(int[] sa) {
    int c1 = 0, t = 0;
    //c2 = 0, c3 = 0, c4 = 0, c5 = 0, 
    for(int i = 0; i < sa.length; i++) {
        c1 = sa[i]; 
        
        for(int j = 0; j < sa.length; j++) {
            if(j == i) {continue;}
            else if(sa[j] == c1 && t == 0){
                t = 1;
                //c2 = sa[j];
            }else if(sa[j] == c1 && t == 1) {
                t = 2;
                //c3 = sa[j];
            }else if(sa[j] == c1 && t == 2) {
                t = 3; 
                //c4 = sa[j];
            }else if(sa[j] == c1 && t == 3) {
                t = 4;
                //c5 = sa[j];
            }
            if(t == 4) {return c1;}
        }
    }
    return 0;
}
    
